#SELECT NAME, COUNT(NAME) AS COUNT  FROM ANIMAL_INS WHERE NAME IS NOT NULL GROUP BY NAME 

#SELECT NAME, COUNT(NAME) AS COUNT 
#FROM (SELECT * FROM ANIMAL_INS WHERE NAME IS NOT NULL) 
#GROUP BY NAME ORDER BY NAME;

SELECT NAME,COUNT AS COUNT FROM(
    SELECT *,COUNT(NAME) AS COUNT FROM ANIMAL_INS WHERE NAME IS NOT NULL GROUP BY NAME
) AS B
WHERE COUNT>=2 ORDER BY NAME;